		"mathoid-texvcjs": "^0.3.10"
	},
	"devDependencies": {
		"coveralls": "^3.1.0",
		"eslint": "8.9.0",
		"istanbul": "^0.4.5",
		"jshint": "^2.13.4",
		"mocha": "^8.1.3",
		"mocha-lcov-reporter": "^1.3.0"
	},
	"bin": {
		"texvcinfo": "./bin/texvcinfo"
